local player = game.Players.LocalPlayer

if game.PlaceId == 3255508650 or game.PlaceId == 11362898741 then
	
	local RunService = game:GetService("RunService")

	local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
	local Window = Library.CreateLib("Rellix", "DarkTheme")
	local Tab = Window:NewTab("Auto")
	local Tabx = Window:NewTab("Hitbox")
	local Tabs = Window:NewTab("Settings")

	local Section = Tab:NewSection("Autos")
	local Sectionx = Tabx:NewSection("BiggerHitbox")
	local Sections = Tabs:NewSection("Settings")


	Section:NewButton("Auto Hit!", "Hit People Fast", function()
		while wait() do
			game:GetService("ReplicatedStorage").remotes.attack:FireServer()
		end
	end)

	Section:NewButton("Auto Block!", "Block Automaticly", function()

		local args = {
			[1] = true
		}

		game:GetService("ReplicatedStorage").remotes.block:FireServer(unpack(args))

	end)


	Sectionx:NewButton("Bigger Hitbox!", "Big hitbox", function()

		local players = game.Players:GetPlayers()
		local speaker = game.Players.LocalPlayer
		game:GetService("RunService").Stepped:Connect(function()
			for i,v in pairs(players) do
				if v ~= speaker then
					if v.Character and v.Character:FindFirstChild('HumanoidRootPart') then
						local sizeArg = 10
						local Size = Vector3.new(sizeArg,sizeArg,sizeArg)
						local Root = v.Character:FindFirstChild('HumanoidRootPart')
						if Root:IsA("BasePart") then
							if not sizeArg == 1 then
								Root.Size = Vector3.new(2,1,1)
								Root.Transparency = 0.2                
							else
								Root.Size = Size
								Root.Transparency = 0.2
							end
						end
					end
				end
			end
		end)
	end)


	Section:NewButton("Anti Fling", "Cant Get Pushed/Flinged", function()
		game:GetService("Players").LocalPlayer.Character.flailing:Destroy()
		game:GetService("Players").LocalPlayer.Character.blocking:Destroy()
	end)



	Sections:NewKeybind("Keybind", "KeybindInfo", Enum.KeyCode.RightShift, function()
		Library:ToggleUI()
	end)



	
	
else
	return
end



































